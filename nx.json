{
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    ".eslintrc.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    }
  },
  "npmScope": "cali",
  "projects": {
    "api": {
      "tags": []
    },
    "api-interfaces": {
      "tags": []
    },
    "pwa": {
      "tags": []
    },
    "pwa-e2e": {
      "implicitDependencies": ["pwa"],
      "tags": []
    }
  },
  "targetDependencies": {
    "build": [
      {
        "projects": "dependencies",
        "target": "build"
      }
    ]
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      },
      "runner": "@nrwl/workspace/tasks-runners/default"
    }
  }
}
