{
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "pwa",
  "projects": {
    "api": {
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "configurations": {
            "production": {
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ],
              "inspect": false,
              "optimization": true
            }
          },
          "options": {
            "assets": ["apps/api/src/assets"],
            "main": "apps/api/src/main.ts",
            "outputPath": "dist/apps/api",
            "tsConfig": "apps/api/tsconfig.app.json"
          },
          "outputs": ["{options.outputPath}"]
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/api"]
        }
      },
      "projectType": "application",
      "root": "apps/api",
      "sourceRoot": "apps/api/src"
    },
    "pwa": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "development": {
              "buildOptimizer": false,
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true
            },
            "production": {
              "budgets": [
                {
                  "maximumError": "1mb",
                  "maximumWarning": "500kb",
                  "type": "initial"
                },
                {
                  "maximumError": "4kb",
                  "maximumWarning": "2kb",
                  "type": "anyComponentStyle"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/pwa/src/environments/environment.ts",
                  "with": "apps/pwa/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "assets": ["apps/pwa/src/favicon.ico", "apps/pwa/src/assets"],
            "index": "apps/pwa/src/index.html",
            "inlineStyleLanguage": "less",
            "main": "apps/pwa/src/main.ts",
            "outputPath": "dist/apps/pwa",
            "polyfills": "apps/pwa/src/polyfills.ts",
            "scripts": [],
            "styles": ["apps/pwa/src/styles.less"],
            "tsConfig": "apps/pwa/tsconfig.app.json"
          },
          "outputs": ["{options.outputPath}"]
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pwa:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/pwa/src/**/*.ts",
              "apps/pwa/src/**/*.html"
            ]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "browserTarget": "pwa:build:development"
            },
            "production": {
              "browserTarget": "pwa:build:production"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/pwa/proxy.conf.json"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/pwa/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/pwa"]
        }
      },
      "prefix": "cali",
      "projectType": "application",
      "root": "apps/pwa",
      "sourceRoot": "apps/pwa/src"
    },
    "pwa-e2e": {
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "configurations": {
            "production": {
              "devServerTarget": "pwa:serve:production"
            }
          },
          "options": {
            "cypressConfig": "apps/pwa-e2e/cypress.json",
            "devServerTarget": "pwa:serve:development",
            "tsConfig": "apps/pwa-e2e/tsconfig.e2e.json"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/pwa-e2e/**/*.{js,ts}"]
          }
        }
      },
      "projectType": "application",
      "root": "apps/pwa-e2e",
      "sourceRoot": "apps/pwa-e2e/src"
    }
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "linter": "eslint",
      "style": "less",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "less"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "style": "less",
      "unitTestRunner": "jest"
    }
  },
  "version": 1
}
