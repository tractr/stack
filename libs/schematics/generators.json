{
  "$schema": "http://json-schema.org/schema",
  "generators": {
    "apps:admin": {
      "description": "Generate a react-admin application",
      "factory": "./src/generators/apps/admin/generator",
      "schema": "./src/generators/apps/admin/schema.json"
    },
    "apps:api": {
      "description": "Generate an api application",
      "factory": "./src/generators/apps/api/generator",
      "schema": "./src/generators/apps/api/schema.json"
    },
    "apps:pwa": {
      "description": "Generate an angular pwa application",
      "factory": "./src/generators/apps/pwa/generator",
      "schema": "./src/generators/apps/pwa/schema.json"
    },
    "create-traxion-workspace": {
      "description": "Take an empty nx workspace configure it to get a ready to go project",
      "factory": "./src/generators/workspace/create=traxion-workspace/generator",
      "schema": "./src/generators/workspace/create=traxion-workspace/schema.json"
    },
    "eslint": {
      "description": "Use the traxion eslint configuration",
      "factory": "./src/generators/configs/eslint/generator",
      "schema": "./src/generators/configs/eslint/schema.json"
    },
    "generate": {
      "description": "Add a target generate to a library",
      "factory": "./src/generators/targets/generate/generator",
      "schema": "./src/generators/targets/generate/schema.json"
    },
    "github-workflows": {
      "description": "github-workflows generator",
      "factory": "./src/generators/github-workflows/generator",
      "schema": "./src/generators/github-workflows/schema.json"
    },
    "initialize-nx-workspace": {
      "description": "Initialize an empty nx workspace to host traxion",
      "factory": "./src/generators/workspace/initialize-nx-workspace/generator",
      "schema": "./src/generators/workspace/initialize-nx-workspace/schema.json"
    },
    "library": {
      "description": "Create a traxion library to be able to use the generation pipeline",
      "factory": "./src/generators/libs/library/generator",
      "schema": "./src/generators/libs/library/schema.json"
    },
    "npm-publish": {
      "description": "Add npm-config target to a library",
      "factory": "./src/generators/targets/npm-publish/generator",
      "schema": "./src/generators/targets/npm-publish/schema.json"
    },
    "prettier": {
      "description": "Use the traxion prettier configuration",
      "factory": "./src/generators/configs/prettier/generator",
      "schema": "./src/generators/configs/prettier/schema.json"
    },
    "prisma": {
      "description": "Add and configure a prisma library",
      "factory": "./src/generators/libs/prisma/generator",
      "schema": "./src/generators/libs/prisma/schema.json"
    },
    "release": {
      "description": "Add a release target to a library",
      "factory": "./src/generators/targets/release/generator",
      "schema": "./src/generators/targets/release/schema.json"
    }
  },
  "name": "schematics",
  "version": "0.0.1"
}
