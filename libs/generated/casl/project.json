{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": ["hapify-templates-casl", "common"],
  "prefix": "tractr",
  "projectType": "library",
  "sourceRoot": "libs/generated/casl/src",
  "tags": [],
  "targets": {
    "generate": {
      "executor": "./dist/libs/schematics:generate",
      "options": {
        "cwd": "libs/generated/casl"
      },
      "outputs": ["libs/generated/casl/src/generated"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/generated/casl/src/**/*.ts",
          "libs/generated/casl/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/generated/casl/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/generated/casl"]
    },
    "ts-morph-generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx ts-node -r tsconfig-paths/register --project ./tsconfig.lib.json ./src/generate.ts",
            "forwardAllArgs": false
          }
        ],
        "cwd": "libs/generated/casl",
        "parallel": false
      }
    }
  }
}
