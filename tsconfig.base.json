        "libs/common/angular-rext-client/src/index.ts"
      ],
      "@cali/common-casl": ["libs/common/casl/src/index.ts"],
      "@cali/common-graphql-client": [
        "libs/common/graphql-client/src/index.ts"
      ],
      "@cali/common-models": ["libs/common/models/src/index.ts"],
      "@cali/common-rest-dtos": ["libs/common/rest-dtos/src/index.ts"],
      "@cali/common-rext-client": ["libs/common/rext-client/src/index.ts"],
